<!DOCTYPE html>
<html>
  	<head>
  		
   		<title>Celuprecios</title>
	    <%= csrf_meta_tags %>

	    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
	    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
	   
	    <%= favicon_link_tag 'logo_navegador.ico' %>
<STYLE TYPE="text/css" MEDIA=screen>
		@media screen and (max-width: 767px) {
		.capa_a_ocultar{
			display:none;
			}
		}
		</STYLE>
  	</head>

	  
	<body >

  <%= render "parcials/navigation_menu" %>
    <%= render "parcials/barrainfo" %>
		
		<div class="row " >
			<div class="col-xs-12 col-md-2 " style="margin-top: 1em;">
				<div class="col-xs-12 div-border-right">
					<div class="center-xs ">
						<h2 class="no-margin top-space ">
							<%= image_tag "categorias.jpg" %>
						</h2>
					</div>
					<div class="col-xs-12 "  >
						<%= render "brands/brand" %>
					<div class="capa_a_ocultar">
            <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
            <script>
                (adsbygoogle = window.adsbygoogle || []).push({
                    google_ad_client: "ca-pub-3774641944437536",
                    enable_page_level_ads: true
                });
            </script>
          </div>
          </div>
				</div>
				
			</div>
			<div class="col-xs-12 col-md-8">
				<div style="width: 90%; margin: 0 auto;">
					<div style="<%= "display: none; " if notice.nil? and alert.nil? %>" >
						<div class="row center-xs letra-times-new-roman">
							<p class="notice col-xs-8 no-margin"><%= notice %></p>
					    	<p class="alert col-xs-8 no-margin"><%= alert %></p>
						</div>
					</div>
 					<div class="row center-xs">
						<div id="results" class="col-xs-8" ></div>
					</div>
					<div id="principal" style="margin-bottom: 3em;">
						<%= render "parcials/login"%>
						<%= yield %>
            <% render "parcials/barra_inferior"%>
					</div>
				</div>
			</div>
			<div class="col-xs-12 col-md-2 center-xs " style="margin-top: 1em; ">
				<div class="capa_a_ocultar">
					<div class="col-xs-12 div-border-left">
					<%= image_tag "Boton_de_Blogs.jpg", class: "bottom-space", style: "margin-left: 20px;"%>
					<%= render "parcials/advertisement" %>
            <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
            <script>
                (adsbygoogle = window.adsbygoogle || []).push({
                    google_ad_client: "ca-pub-3774641944437536",
                    enable_page_level_ads: true
                });
            </script>

        </div>
					
				</div>
				
			</div>
		</div>
    </body>

	
</html>
